{
  "name": "llm-council",
  "version": "0.1.3",
  "description": "Multi-LLM council that collects responses, peer-ranks them, and synthesizes a final answer",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "example": "tsx examples/ollama.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "llm",
    "ai",
    "council",
    "openrouter",
    "ollama",
    "multi-model"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/himalayanZephyr/llm-council.git"
  },
  "bugs": {
    "url": "https://github.com/himalayanZephyr/llm-council/issues"
  },
  "homepage": "https://github.com/himalayanZephyr/llm-council#readme",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  }
}
